{
	"name" : "stinger-soft/doctrine-entity-search-bundle",
	"type" : "symfony-bundle",
	"description" : "Doctrine Layer to index and search entities",
	"keywords" : [
		"Search"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\DoctrineEntitySearchBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"license" : "Apache-2.0",
	"require" : {
		"php" : "^8.1",
		"stinger-soft/entity-search-bundle" : "^2@dev",
		"knplabs/knp-paginator-bundle" : "^6.0",
		"stinger-soft/php-commons" : "^1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5",
		"php-coveralls/php-coveralls" : "^2.5",
		"symfony/phpunit-bridge" : "^5.0||^6.0"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.2.x-dev",
			"dev-develop" : "2.0.x-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"ocramius/package-versions": false
		}
	}
}
